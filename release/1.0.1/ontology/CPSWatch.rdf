        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Deviation threshold of a CPSWatch:EventMonitoringSensor that needs to be exceeded to save the current value.</rdfs:comment>
        <rdfs:label>hasDeviationThreshold</rdfs:label>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
