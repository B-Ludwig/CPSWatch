        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Update rate in seconds (s) that the data is stored.</rdfs:comment>
        <rdfs:label>hasSampleRate</rdfs:label>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/CPSWatch#hasSensorID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/CPSWatch#hasSensorID">
        <rdfs:domain rdf:resource="https://w3id.org/CPSWatch#MonitoringSensor"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment xml:lang="en">The unique ID of a CPSWatch#MonitoringSensor.</rdfs:comment>
        <rdfs:label xml:lang="en">hasID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/CPSWatch#hasSystemID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/CPSWatch#hasSystemID">
        <rdfs:domain rdf:resource="https://w3id.org/CPSWatch#MonitoredSystem"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment xml:lang="en">The unique ID of a CPSWatch#MonitoredSystem.</rdfs:comment>
        <rdfs:label xml:lang="en">hasID</rdfs:label>
    </owl:DatatypeProperty>
    


