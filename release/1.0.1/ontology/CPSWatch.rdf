        <rdfs:domain rdf:resource="https://w3id.org/CPSWatch#MonitoringSensor"/>
        <rdfs:range rdf:resource="https://w3id.org/CPSWatch#MonitoringSensor"/>
        <rdfs:comment xml:lang="en">Connection between two CPSWatch:MonitoringSensor to state that the observations of these sensors are with a positive correlation.</rdfs:comment>
        <rdfs:label xml:lang="en">hasPositiveCorrelation</rdfs:label>
    </owl:ObjectProperty>
    


